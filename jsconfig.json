{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "target": "ES2024",
        "jsx": "react",
        "allowImportingTsExtensions": true,
        "checkJs": false,
        "strictFunctionTypes": true,
        "strictNullChecks": true
    },
    "include": [
        "**/*.js"
    ],
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}
